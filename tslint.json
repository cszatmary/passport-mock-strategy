{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "prettier": true,
    "no-var-requires": false,
    "no-console": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-implicit-dependencies": [true, "dev"]
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
